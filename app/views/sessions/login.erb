<h2>Log In </h2>

<% if @error %>
    <p> <%= @error %> </p>
<% end %>

<form action="/login" method=post>
 <input type="text" placeholder="Username" name= "username">
 <input type="password" placeholder="Password" name= "password">
 <input type="submit" value="Log In">
</form>
<br><br>
<a href="/signup">Sign Up</a>